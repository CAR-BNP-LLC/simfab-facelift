    "dev:build": "tsc && npm start",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "connect-sqlite3": "^0.9.13",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
